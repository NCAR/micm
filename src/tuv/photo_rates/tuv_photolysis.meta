[ccpp-arg-table]
  name = tuv_photolysis_init
  type = scheme
[realkind]
  standard_name = phys_real_kind
  long_name = physics real kind
  units = 1
  dimensions = ()
  type = integer
  intent = in
  optional = F
[tuv_n_wavelen]
  standard_name = tuv_n_actinic_flux_wavelengths
  units = count
  dimensions = ()
  type = integer
  intent = out
[tuv_n_phot]
  standard_name = tuv_n_photodecomp_reactions
  units = count
  dimensions = ()
  type = integer
  intent = out
[errmsg]
  standard_name = ccpp_error_message
  long_name = CCPP error message
  units = 1
  dimensions = ()
  type = character
  kind = len=512
  intent = out
  optional = F
[errflg]
  standard_name = ccpp_error_flag
  long_name = CCPP error flag
  units = flag
  dimensions = ()
  type = integer
  intent = out
  optional = F

########################################################################
[ccpp-arg-table]
  name = tuv_photolysis_run
  type = scheme
[nlev]
  standard_name = vertical_level_dimension
  long_name = number of column layers
  units = count
  dimensions = ()
  type = integer
  intent = in
  optional = F
[temp]
  standard_name = layer_temperature
  long_name = mid-point layer temperature
  units = K
  dimensions = (vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = in
  optional = F
[press_mid]
  standard_name = layer_pressure
  long_name = mid-point layer pressure
  units = Pa
  dimensions = (vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = in
  optional = F
[radfld]
  standard_name = actinic_photon_fluxes
  long_name = actinic photon fluxes
  units = cm-2 sec-1
  dimensions = (tuv_n_actinic_flux_wavelengths,vertical_level_dimension)
  type = real
  kind = kind_phys
  intent = in
  optional = F
[srb_o2_xs]
  standard_name = o2_xsect
  long_name = O2 effective cross section
  units = cm2
  dimensions = (tuv_n_actinic_flux_wavelengths,vertical_level_dimension)
  type = real
  kind = kind_phys
  intent = in
  optional = F
[tuv_prates]
  standard_name = photolysis_rates_col
  long_name = photolysis rates column
  units = s-1
  dimensions = (vertical_level_dimension,number_of_tuv_jrates)
  type = real
  kind = kind_phys
  intent = out
  optional = F
[errmsg]
  standard_name = ccpp_error_message
  long_name = CCPP error message
  units = 1
  dimensions = ()
  type = character
  kind = len=512
  intent = out
  optional = F
[errflg]
  standard_name = ccpp_error_flag
  long_name = CCPP error flag
  units = flag
  dimensions = ()
  type = integer
  intent = out
  optional = F

