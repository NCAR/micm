[ccpp-arg-table]
  name = molec_ox_xsect_init
  type = scheme
[errmsg]
  standard_name = ccpp_error_message
  long_name = CCPP error message
  units = 1
  dimensions = ()
  type = character
  kind = len=*
  intent = out
[errflg]
  standard_name = ccpp_error_flag
  long_name = CCPP error flag
  units = flag
  dimensions = ()
  type = integer
  intent = out

########################################################################
[ccpp-arg-table]
  name = molec_ox_xsect_run
  type = scheme
[nlyr]
  standard_name = vertical_layer_dimension
  long_name = number of column layers
  units = count
  dimensions = ()
  type = integer
  intent = in
[zen]
  standard_name = solar_zenith
  long_name = solar zenith angle
  units = degrees
  dimensions = ()
  type = real
  kind = kind_phys
  intent = in
[alt]
  standard_name = layer_altitude
  long_name = mid-point layer altitude
  units = m
  dimensions = (vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = in
[temp]
  standard_name = layer_temperature
  long_name = mid-point layer temperature
  units = K
  dimensions = (vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = in
[press_mid]
  standard_name = layer_pressure
  long_name = mid-point layer pressure
  units = Pa
  dimensions = (vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = in
[press_top]
  standard_name = top_interface_pressure
  long_name = pressure at the top of the model
  units = Pa
  dimensions = ()
  type = real
  kind = kind_phys
  intent = in
[o2vmr]
  standard_name = o2_vmr_col
  long_name = O2 volume mixing ratio column
  units = mole/mole
  dimensions = (vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = in
[dto2]
  standard_name = o2_optical_depth
  long_name = optical depth due to O2 absorption
  units = cm
  dimensions = (vertical_layer_dimension,tuv_n_actinic_flux_wavelengths)
  type = real
  kind = kind_phys
  intent = out
[srb_o2_xs]
  standard_name = o2_xsect
  long_name = O2 effective cross section
  units = cm2
  dimensions = (tuv_n_actinic_flux_wavelengths,vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = out
[errmsg]
  standard_name = ccpp_error_message
  long_name = CCPP error message
  units = 1
  dimensions = ()
  type = character
  kind = len=*
  intent = out
[errflg]
  standard_name = ccpp_error_flag
  long_name = CCPP error flag
  units = flag
  dimensions = ()
  type = integer
  intent = out

