[ccpp-arg-table]
  name = tuv_radiation_transfer_init
  type = scheme
[realkind]
  standard_name = phys_real_kind
  long_name = physics real kind
  units = 1
  dimensions = ()
  type = integer
  intent = in
[errmsg]
  standard_name = ccpp_error_message
  long_name = CCPP error message
  units = 1
  dimensions = ()
  type = character
  kind = len=*
  intent = out
[errflg]
  standard_name = ccpp_error_flag
  long_name = CCPP error flag
  units = flag
  dimensions = ()
  type = integer
  intent = out

########################################################################
[ccpp-arg-table]
  name = tuv_radiation_transfer_run
  type = scheme
[nlayer]
  standard_name = vertical_layer_dimension
  long_name = number of column layers
  units = count
  dimensions = ()
  type = integer
  intent = in
[tuv_n_wavelen]
  standard_name = tuv_n_actinic_flux_wavelengths
  units = count
  dimensions = ()
  type = integer
  intent = in
[zenith]
  standard_name = solar_zenith
  long_name = solar zenith angle
  units = degrees
  dimensions = ()
  type = real
  kind = kind_phys
  intent = in
[albedo]
  standard_name = surface_albedo
  long_name = surface albedo
  units = 1
  dimensions = ()
  type = real
  kind = kind_phys
  intent = in
[press_mid]
  standard_name = layer_pressure
  long_name = mid-point layer pressure
  units = Pa
  dimensions = (vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = in
[press_top]
  standard_name = top_interface_pressure
  long_name = pressure at the top of the model
  units = Pa
  dimensions = ()
  type = real
  kind = kind_phys
  intent = in
[alt]
  standard_name = layer_altitude
  long_name = mid-point layer altitude
  units = m
  dimensions = (vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = in
[temp]
  standard_name = layer_temperature
  long_name = mid-point layer temperature
  units = K
  dimensions = (vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = in
[o3vmr]
  standard_name = o3_vmr_col
  long_name = O3 volume mixing ratio column
  units = mole/mole
  dimensions = (vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = in
[so2vmr]
  standard_name = so2_vmr_col
  long_name = SO2 volume mixing ratio column
  units = mole/mole
  dimensions = (vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = in
[no2vmr]
  standard_name = no2_vmr_col
  long_name = NO2 volume mixing ratio column
  units = mole/mole
  dimensions = (vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = in
[cldfrc]
  standard_name = cloud_fraction
  long_name = cloud fraction
  units = none
  dimensions = (vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = in
[cldwat]
  standard_name = cloud_water_dens
  long_name = cloud water mass density
  units = g/m3
  dimensions = (vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = in
[dto2]
  standard_name = o2_optical_depth
  long_name = optical depth due to O2 absorption
  units = cm
  dimensions = (vertical_layer_dimension,tuv_n_actinic_flux_wavelengths)
  type = real
  kind = kind_phys
  intent = in
[radfld]
  standard_name = actinic_photon_fluxes
  long_name = actinic photon fluxes
  units = cm-2 sec-1
  dimensions = (tuv_n_actinic_flux_wavelengths,vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = out
[errmsg]
  standard_name = ccpp_error_message
  long_name = CCPP error message
  units = 1
  dimensions = ()
  type = character
  kind = len=*
  intent = out
[errflg]
  standard_name = ccpp_error_flag
  long_name = CCPP error flag
  units = flag
  dimensions = ()
  type = integer
  intent = out
